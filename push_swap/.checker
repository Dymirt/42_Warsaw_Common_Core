i=1
while [ $i -le 100 ]
do
    ARG=$(shuf -i 0-14900 -n 5 | tr '\n' ' ');
	echo "ARG: $ARG";
    RESULT=$(./push_swap $ARG | wc -l);
	echo "RESULT: $RESULT";
    AVERAGE=$(($AVERAGE + $RESULT));
	i=$((i + 1))
done

AVERAGE=$(echo "scale=2; $AVERAGE / 100" | bc)

echo "AVERAGE: $AVERAGE"

# 3 numbers 1.47 `average`
# 5 numbers 8.3 `average`
# 100 numbers 2252499.90 `average`
# 500 numbers  `average`
